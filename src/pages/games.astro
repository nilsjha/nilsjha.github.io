---
import Layout from "@layouts/Layout.astro";
import Banner from "@components/Banner.astro";
import MenuEntry from "@components/MenuEntry.astro";
import Footer from "@components/Footer.astro";
import MenuSpacer from "@components/MenuSpacer.astro";
import MenuList from "@components/MenuList.astro";
import Screen from "@components/Screen.astro";
---

<Layout title="nilsjha.github.io">
    <Banner
      org="Games from"
      deptChar="IA"
      dept="Internet Archive"
      title="Games"
      help1=""
      help2=""
    />
    <Screen>
    <MenuList>
      <MenuEntry
        href="/"
        title="Home"
        body="RETURN to the main page"
      />
      <MenuSpacer />
      <MenuEntry href="/games/space" title="Space Commanders" body="Space Invaders Clone, 1983" />
      <MenuEntry href="/games/digger" title="Digger" body="Maze game by Windmill Software, 1983" />
      <MenuEntry href="/games/dave" title="Dave" body="Dangerous Dave, Plattform Game by John Romero/1988" />
      <MenuEntry href="/games/outrun" title="Outrun2" body="The original arcade from Sega, 1986" />
      <MenuSpacer />
      <MenuEntry
        href="https://archive.org/details/softwarelibrary_msdos_games"
        title="More games"
        body="Hosted at Internet Archive"
      />
    </MenuList>
    </Screen>
  <Footer home={true} />
</Layout>
