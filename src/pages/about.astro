
---
import Layout from "@layouts/Layout.astro";
import Banner from "@components/Banner.astro";
import MenuEntry from "@components/MenuEntry.astro";
import Footer from "@components/Footer.astro";
import MenuSpacer from "@components/MenuSpacer.astro";
import MenuList from "@components/MenuList.astro";
import Screen from "@components/Screen.astro";
---

<Layout title="nilsjha.github.io">
    <Banner
      org="Kommunedata"
      orgChar="KD"
      dept="Vestlandet"
      title="About"
      help1=""
      help2=""
    />
    <div class="flex flex-row items-center justify-center">
        <div class="w-2/4">Hello!</div>
    </div>
    <Screen>
    <MenuList>
      <MenuEntry
        href="/"
        title="Hovudmeny (Home)"
        body="RETURNER til forrige side"
      />
      <MenuEntry
        href="https://en.wikipedia.org/wiki/IBM_Personal_Computer"
        title="What is this?"
        body="Honouring history that made today possible"
      />
      <MenuSpacer />
    </MenuList>
    </Screen>
  <Footer home={true} />
</Layout>
